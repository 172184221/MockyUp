          "type": "string",
          "example": "SUCCESS"
        },
        "Transaction_amount": {
          "type": "number",
          "example": 12500
        },
        "Transaction_date": {
          "type": "string",
          "example": "2019-02-19"
        },
        "Transaction_time": {
          "type": "string",
          "example": "13:41:15"
        },
        "Transaction_type": {
          "type": "string",
          "example": "TRANSFER"
        },
        "Transaction_type_id": {
          "type": "integer",
          "example": 12
        },
        "Ui_type": {
          "type": "integer",
          "example": 1
        }
