    "Category": {
      "type": "object",
      "properties": {
        "ID": {
          "type": "integer",
          "format": "int64",
          "example": 360001360831
        },
        "Name": {
          "type": "string",
          "example": "Paybill"
        },
        "Description": {
          "type": "string",
          "format": "url",
          "description": "link to icon",
          "example": "https://cdnjs.cloudflare.com/ajax/libs/ionicons/4.4.8/collection/icon/svg/ios-boat.svg"
        }
      }
    },
    "Subcategory": {
      "type": "object",
