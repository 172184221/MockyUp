        },
        "Category_id": {
          "type": "integer",
          "example": 6
        },
        "Category_name": {
          "type": "string",
          "example": "others"
        },
        "Desc1": {
          "type": "string",
          "example": "Transfer Ke Lol Lol"
        },
        "Desc2": {
          "type": "string",
          "example": "Transfer Keluar"
        },
        "Desc3": {
          "type": "string",
          "example": "Lol Lol"
        },
        "Icon_url": {
          "type": "string",
          "example": "1"
        },
        "Merchant_id": {
          "type": "string",
          "example": "1"
        },
        "Merchant_invoice": {
          "type": "string",
          "example": "TRF-e7e9857b3d1103db8f665068e03d5308ba2ecd10"
        },
        "Merchant_name": {
          "type": "string",
          "example": "OVO"
        },
        "Name": {
          "type": "string",
          "example": "nessa"
